KISSY.add("pcm/pages/movie/index",function(i,t){function e(){this.init()}var n=t.all;return i.augment(e,{init:function(){var i=this;i.movieTab(".tab-control-item",".tab-movie-list")},movieTab:function(t,e){n(t).item(0).addClass("current"),n(e).item(0).css({display:"inline-block"}),i.each(n(t),function(i,t){n(i).on("click",function(i){i.preventDefault(),n(i.target).addClass("current"),n(i.target).siblings().removeClass("current"),n(e).item(t).show().siblings().hide()})})}}),e},{requires:["node"]});